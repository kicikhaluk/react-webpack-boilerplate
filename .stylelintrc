{
  "extends": "stylelint-config-standart",
  "rules": {
    "selector-class-pattern": null,
    "function-no-unknown": null,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "use",
          "function",
          "if",
          "each",
          "include",
          "mixin",
          "return",
          "extend"
        ]
      }
    ]
  }
}
